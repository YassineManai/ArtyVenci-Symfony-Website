{% extends 'Admin.html.twig' %}

{% block title %}Event index{% endblock %}

{% block content %}
<div class="row">
						<div class="col-12 col-lg-8 col-xxl-9 d-flex">
							<div class="card flex-fill">
								<div class="card-header">

									<h5 class="card-title mb-0">Latest Participations</h5>
								</div>
								<table class="table table-hover my-0">
									<thead>
										<tr>
											<th>Id</th>
											<th class="d-none d-xl-table-cell">Event</th>
											<th class="d-none d-xl-table-cell">User</th>
											
										</tr>
									</thead>
									<tbody>
                                     {% for p in participations %}
										<tr>
											<td>{{p.idParticipation}}</td>
											<td class="d-none d-xl-table-cell">{{p.idEvent.eName}}</td>
											<td class="d-none d-xl-table-cell">{{p.idUser.username}}</td>
											<td></td>
											
										</tr>
										{% else %}
            <tr>
                <td colspan="7">no records found</td>
            </tr>
        {% endfor %}
									</tbody>
								</table>
							</div>
						</div>
                        
					</div>


 
						
{% endblock %}
